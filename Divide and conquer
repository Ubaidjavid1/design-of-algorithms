#include <stdio.h>

void demo(int a[], int start, int end) {
  int middle;

  if(start < end) {
    middle = (start+end)/2;
    demo(a, start, middle);
    demo(a, middle+1, end);
  }
  else {
    a[start] = a[start]+1;
  }
}

int main() {
  int a[] = {4, 8, 1, 3, 10, 9, 2, 11, 5, 6};
  demo(a, 0, 9);

  //printing array
  int i;
  for(i=0; i<10; i++) {
    printf("%d ",a[i]);
  }
  printf("\n");
  return 0;
}
